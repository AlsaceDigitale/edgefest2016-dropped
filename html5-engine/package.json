    "html-webpack-plugin": "^2.18.0",
    "imports-loader": "^0.6.5",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.7.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "ansi-to-html": "^0.4.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
